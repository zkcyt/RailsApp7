<h1>Blogposts#index</h1>
<p>投稿記事の一覧</p>
<table>
<tr>
  <th>Id</th><th>title</th><th>read</th>
  <th>genre</th><th colspan="2"></th>
</tr>

<% @data.each do |obj| %>
<tr>
  <td><%= obj.id %></td>
  <td><%= obj.title %></td>
  <td><%= obj.read %></td>
  <td><%= obj.bloggenre.name %></td>
  <td><%= link_to("Edit", "/blogposts/#{obj.id}") %></td>
  <td><%= link_to("Delete", "/blogposts/delete/#{obj.id}") %></td>
</tr>
<% end %>
</table>
<p><%= link_to("新規作成", "/blogposts/add" ) %></p>
